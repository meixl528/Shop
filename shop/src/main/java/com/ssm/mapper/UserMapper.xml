<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTDMapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssm.mapper.UserMapper">
	<!-- <cache type="com.ssm.util.LoggingRedisCache"></cache> -->
	<select id="getUserList" resultType="com.ssm.dto.User">
		select * from user
	</select>
	
	<insert id="addUser" parameterType="com.ssm.dto.User">
		insert into User (name,pass,telephone,address) values (#{name},#{pass},#{telephone},#{address})
	</insert>
	
	<delete id="deleteAllUser">
		delete from User where id > 20
	</delete>
	
	<select id="getUserListLimit" resultType="com.ssm.dto.User">
		SELECT * FROM USER ORDER BY id DESC LIMIT 20 
	</select>
</mapper>